pre
  height 90px
  max-height 90px
  overflow scroll

:focus
  outline none !important

*
  margin 0px
  padding 0px

html, body
  -webkit-tap-highlight-color rgba(0, 0, 0, 0)
  -webkit-font-smoothing antialiased
  -webkit-user-select none
  position relative
  top 0px
  right 0px
  bottom 0px
  left 0px
  height 100%
  width 100%
  max-width 100%
  outline 0px
  overflow hidden !important
  color $text1
  background $grey1
  font-family 'Brandon Reg'
  font-size 20px

a
  &:active, &:hover, &:link, &:visited
    color inherit
    text-decoration none

p
  display inline

ul
  padding-left 0px

li
  list-style-type none
  text-align left

h1, h2, h3, h4, h5, h6
  margin 0px
  padding 0px
